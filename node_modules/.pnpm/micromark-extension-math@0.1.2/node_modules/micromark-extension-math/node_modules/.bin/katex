#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kerry/Code/node/md-to-notion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kerry/Code/node/md-to-notion/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../katex@0.12.0/node_modules/katex/cli.js" "$@"
else
  exec node  "$basedir/../../../../../katex@0.12.0/node_modules/katex/cli.js" "$@"
fi
